                        {{!-- <button class="btn-style btn-primary">Read More</button> --}}
                    </div>
                    <div class="col-md-6 col-12 hero-text order-md-1 order-0">
                        <figure>
                            <img src="images/img2.svg" alt="hihi" align="right">
                        </figure>
                    </div>
                </div>
                
            </div>
        </div>
    </div>
    {{!-- about us --}}
    <div class="container-fluid mt-5 about-style" id="aboutid">
        <div class="row">
            <div class="col-md-10 col-12 mx-auto">
                <h1 class="text-center main-heading">ABOUT US</h1>
                <p class="text-center sub-heading">WE PROVIDE SOLUTIONS</p>
                <div class = "row">
                    <div class="col-md-6 col-12 hero-text ">
                        <figure>
                            <img src="images/img4.jpg" height = "500" width="700" alt="hihi" align = "left">
                        </figure>
                    </div>

                    <div class="col-md-6 col-12 hero-text d-flex justify-content-center align-items-center flex-column">
                        <h1>ONE PLACE FOR ALL PROBLEMS</h1>
                        <h1>WE DONT JUDGE, WE FIX</h1>
                        <P>Best in the<span class="bold-style"> GAME</span></P>
                        {{!-- <button class="btn-style btn-primary">Read More</button> --}}
                    </div>
                </div>
            </div>
        </div>
    </div>

    {{!-- service --}}
    {{>service}}

